{
  "name": "model",
  "version": "1.6.6",
  "main": "src/main.js",
  "scripts": {
    "start": "npx tailwindcss build src/styles/tailwind.css -o src/styles/output.css && electron .",
    "build:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/output.css --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.example.myapp",
    "publish": [
      {
        "provider": "github",
        "owner": "mwkim197",
        "repo": "model",
        "releaseType": "release"
      }
    ],
    "productName": "model",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "sign": false,
      "artifactName": "model-setup-${version}.exe"
    }
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.1.0",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9",
    "electron-reload": "^2.0.0-alpha.1",
    "tailwindcss": "^3.4.15"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "aws-cli": "^0.0.2",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "electron-log": "^5.2.4",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.9",
    "express": "^4.21.1",
    "multer": "^1.4.5-lts.1",
    "serialport": "^12.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-scrollbar-hide": "^1.1.7"
  }
}
